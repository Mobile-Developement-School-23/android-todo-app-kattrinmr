<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TextViewTitleLargeStyle">
        <item name="android:textColor">@color/label_primary</item>
        <item name="android:textSize">@dimen/title_large</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TextViewTitleStyle">
        <item name="android:textColor">@color/label_tertiary</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="FloatingActionButtonCircleStyle" parent="ShapeAppearance.Material3.SmallComponent">
        <item name="cornerSize">100%</item>
    </style>

    <style name="ThemeOverlay.App.CheckBox" parent="">
        <item name="colorOnSurface">@color/label_tertiary</item>
        <item name="colorSecondary">@color/green</item>
    </style>

    <style name="Widget.App.CheckBox" parent="Widget.MaterialComponents.CompoundButton.CheckBox">
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.CheckBox</item>
    </style>

    <style name="TextViewBodyStyle">
        <item name="android:textColor">@color/label_primary</item>
        <item name="android:textSize">@dimen/text_body</item>
    </style>

    <style name="TextViewSubheadStyle">
        <item name="android:textColor">@color/label_tertiary</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>